{{CSS_CLASS}} g.system-labels text { font-size: {{FONTSIZE}}px; text-shadow: #333 0 0 {{SHADOW0AREA}}px, #333 0 0 {{SHADOW1AREA}}px, #333 0 0 {{SHADOW2AREA}}px, #333 0 0 {{SHADOW3AREA}}px; }
{{CSS_CLASS}} g.system-labels text.abandoned { fill: #999; font-style: italic; }
{{CSS_CLASS}} g.system-labels text.undiscovered { fill: #999; font-style: italic; }
{{CSS_CLASS}} g.system-labels text tspan.sup { font-size: {{FONTSIZESUP}}px; }
{{CSS_CLASS}} g.system-labels .additions { fill: #bbb; font-size: {{FONTSIZEADDITIONS}}px; }
{{CSS_CLASS}} g.system-labels .additions tspan.apocryphal { fill: #c00; }
{{CSS_CLASS}} g.system-labels .apocryphal tspan.sup { fill: #c00 }
