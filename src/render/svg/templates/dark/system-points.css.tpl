{{PREFIX}}g.systems .system, g.systems .cluster { fill: #000; stroke: #ccc; stroke-width: .25; } /* default color */
{{PREFIX}}g.systems .system-decoration { fill: none; stroke: #ccc; stroke-width: .25; }
{{PREFIX}}g.systems .system.U, g.systems .cluster.U { stroke: #777; fill: #777; }
{{PREFIX}}g.systems .system.I, g.systems .cluster.I { stroke: #555; }
{{PREFIX}}g.systems .system.D, g.systems .cluster.D { fill: #c00; }
{{PREFIX}}g.systems .system.hidden { stroke: #000; stroke-dasharray: .4 .3; }
{{PREFIX}}g.systems .cluster { fill-opacity: 0.25; }
