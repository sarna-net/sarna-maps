.border-labels .candidate { fill: red; }
.border-labels .candidate.inside { fill: green; }
.border-labels .candidate-index { font-style: normal; text-anchor: middle; }
.border-labels .candidate-control-line { stroke: green; stroke-width: .3px; }
.border-labels .candidate-perp-edge { stroke: darkgreen; stroke-width: .3px; }
.border-labels .candidate-rect { stroke: #333; stroke-width: .3px; fill: none; }
.border-labels .candidate-rect.p90 { fill: #0a0; }
.border-labels .candidate-rect.p80 { fill: #390; }
.border-labels .candidate-rect.p70 { fill: #580; }
.border-labels .candidate-rect.p60 { fill: #880; }
.border-labels .candidate-rect.p50 { fill: #aa0; }
.border-labels .candidate-rect.p40 { fill: #a80; }
.border-labels .candidate-rect.p30 { fill: #a50; }
.border-labels .candidate-rect.p20 { fill: #a30; }
.border-labels .candidate-rect.p10 { fill: #a00; }
.border-labels .candidate-rect.p00 { fill: #333; }
.border-labels .candidate-rect.disqualified { stroke-width: .5px; stroke: #f00; stroke-dasharray: 1 1; }
.border-labels .candidate-overlap { stroke: none; fill: #f00; }
